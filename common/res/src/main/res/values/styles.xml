<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 默认水平分割线样式 -->
    <style name="_res_Line_HorizontalStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/_res_app_default_line</item>
    </style>

    <!-- 默认垂直分割线样式 -->
    <style name="_res_Line_VerticalStyle">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/_res_app_default_line</item>
    </style>

    <!-- 默认文本框样式 -->
    <style name="_res_EditText_Style">
        <item name="android:background">@null</item>
        <item name="android:textSize">@dimen/_res_sp_15</item>
        <item name="android:textColorHint">@color/_res_app_default_text_hint</item>
        <item name="android:textColorHighlight">@color/_res_app_default_text_hint</item>
        <item name="android:textColor">#333333</item>
        <item name="android:paddingTop">@dimen/_res_dp_10</item>
        <item name="android:paddingBottom">@dimen/_res_dp_10</item>
        <item name="android:paddingStart">@dimen/_res_dp_10</item>
        <item name="android:paddingEnd">@dimen/_res_dp_10</item>
    </style>

</resources>