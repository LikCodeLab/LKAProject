<?xml version="1.0" encoding="utf-8"?>
<!-- 直角按钮样式 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 禁用状态 -->
    <item android:drawable="@color/_res_app_default_button_disable" android:state_enabled="false" />

    <!-- 按压状态 -->
    <item android:drawable="@color/_res_app_default_button_pressed" android:state_pressed="true" />

    <!-- 焦点状态 -->
    <item android:drawable="@color/_res_app_default_button_pressed" android:state_focused="true" />

    <!-- 默认状态 -->
    <item android:drawable="@color/_res_app_default_accent" />

</selector>